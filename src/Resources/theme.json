{
  "name": "TemplateTraining",
  "author": "Shopware AG",
  "views": [
     "@Storefront",
     "@Plugins",
     "@TemplateTraining"
  ],
  "style": [
    "app/storefront/src/scss/overrides.scss",
    "@Storefront",
    "app/storefront/src/scss/base.scss"
  ],
  "script": [
    "@Storefront",
    "app/storefront/dist/storefront/js/template-training.js"
  ],
  "asset": [
    "@Storefront",
    "app/storefront/src/assets"
  ],
  "config": {
    "fields": {
      "sw-logo-desktop": {
        "value": "app/storefront/src/assets/logo_dark.png",
        "type": "media"
      },
      "sw-logo-tablet": {
          "value": "app/storefront/src/assets/logo_dark.png",
          "type": "media"
      },
      "sw-logo-mobile": {
        "value": "app/storefront/src/assets/logo_dark.png",
        "type": "media"
      },
      "sw-logo-share": {
        "value": "app/storefront/src/assets/logo_dark.png",
        "type": "media"
      }
    }
  }
}
